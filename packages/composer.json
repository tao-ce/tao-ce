{
    "name": "oat-ce/main",
    "description": "TAO Open source",
    "type": "project",
    "license": "GPL-2.0-only",
    "repositories": [
        {
            "type": "path",
            "url": "./oat-sa/lib-em-php-client"
        },
        {
            "type": "path",
            "url": "./oat-sa/lib-em-php-lti-client"
        },
        {
            "type": "path",
            "url": "./oat-sa/lib-em-php-lti-events"
        },
        {
            "type": "path",
            "url": "./oat-sa/bundle-em-client"
        }
    ],
    "require": {
        "wikimedia/composer-merge-plugin": "^2.1",
        "oat-sa/bundle-em-client": "*",
        "oat-sa/lib-em-php-client": "*",
        "oat-sa/lib-em-php-lti-client": "*",
        "oat-sa/lib-em-php-lti-events": "*"
    },
    "minimum-stability": "dev",
    "extra": {
        "merge-plugin": {
            "include": [
                "general-composer.json"
            ],
            "recurse": true,
            "replace": true,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        }
    }
}
