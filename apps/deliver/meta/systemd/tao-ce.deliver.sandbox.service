[Unit]

[Service]
Type=simple
ExecStart=/bin/sh -c "cd ${TAO_CE_OPT}/deliver/sandbox && exec /usr/local/libexec/nvm/nvm-exec npm start"
EnvironmentFile=/etc/environment
EnvironmentFile=/etc/tao-ce/setup/envs/dir.env
EnvironmentFile=/etc/tao-ce/setup/envs/svc.env
EnvironmentFile=/etc/tao-ce/setup/envs/deliver/sandbox.env
Restart=always
RestartSec=5s

[Install]
WantedBy=tao-ce.target